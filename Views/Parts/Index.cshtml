@model IEnumerable<ShadFrame.Models.Parts>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Part_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Part_Cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Part_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Part_Cost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PartsId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PartsId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PartsId })
        </td>
    </tr>
}

</table>
